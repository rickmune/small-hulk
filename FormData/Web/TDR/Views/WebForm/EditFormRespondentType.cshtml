@model TDR.WEB.LIB.DTOS.Forms.FormRespondentTypeDTO

@{
    ViewBag.Title = "Edit Form Respondent Type";
   
}

<div class="page-header">
    <h1>Edit Form Respondent Type </h1>
</div>
<div>
    @Html.ActionLink("Back", "RespondentType",new {formId=Model.FormId},new { @class="btn btn-default"})
</div>
@using (Html.BeginForm("EditFormRespondentType", "WebForm", FormMethod.Post, new { @class = "form-horizontal", @role = "form" }))
{
     @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    @Html.HiddenFor(model=>model.FormId)
     @Html.HiddenFor(model=>model.Id)

    <div class="form-group">
        <label for="Username" class="col-lg-2 control-label">Name</label>
        <div class="col-lg-5">
            @Html.TextBoxFor(model => model.Name,new {@placeholder="Name",@class="form-control"})
            @Html.ValidationMessageFor(model => model.Name )
            @* <input type="text" class="form-control" id="inputUsername" placeholder="Username">*@
        </div>
    </div>
    <div class="form-group">
        <label for="Value" class="col-lg-2 control-label">Value</label>
        <div class="col-lg-5">
            @Html.TextBoxFor(model => model.Value,new {@placeholder="Value",@class="form-control"})
            @Html.ValidationMessageFor(model => model.Value )
            @*<input type="text" class="form-control" id="inputFullname" placeholder="Full Name">*@
        </div>
    </div>
    <div class="form-group">
        <label for="Code" class="col-lg-2 control-label">Code</label>
        <div class="col-lg-5">
            @Html.TextBoxFor(model => model.Code,new {@placeholder="Code",@class="form-control"})
            @Html.ValidationMessageFor(model => model.Code )
            @*<input type="text" class="form-control" id="inputFullname" placeholder="Full Name">*@
        </div>
    </div>
   
    
   
    <div class="form-group">
    <div class="col-lg-offset-2 col-lg-10">
      <button type="submit" class="btn btn-default">Save</button>
    </div>
  </div>
}



 @section scripts
  {
       @Scripts.Render("~/bundles/jqueryval")
    <script type="text/javascript" >
        window.pageload = function () {
          
        }
    </script>
}